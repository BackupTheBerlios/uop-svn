entity a
	def int x
	def set_x(int value)
		x = value
	end
end

entity start
	def start()
		def element e = a.new()
		e.on_property_changed += alterado
		e.set_x(12)
	end
	def alterado(string prop, string old, string new)
		io.writeln("A propriedade " + prop + " foi alterada de " + old + " para " + new)
//		io.writeln("A propriedade ", prop, " foi alterada de ", old, " para ", new)
	end
end

