import datetime;

entity servico
	service soma(int x, int y) [int]
		return x+y;
	end
	method constructor()
		{"service"}.bind("Provider");
		{"service"}.service += soma(int,int);
	end
end

entity start
	method constructor()
		var element s = servico.new();
		s.servico();
		datetime.sleep(100);
	end
end

