entity reservatorio
	def event cheio
	def int litros
		get return litros
		set litros = value
			if (litros >= 1000)
				litros = 1000
				raise cheio
			end
		end
	end
end

entity start
	def start()
		def element agua = reservatorio.new()
		agua.cheio += reservatorio_cheio
		agua.litros = 100
		agua.litros = 1000
	end
	def reservatorio_cheio()
		io.writeln("O reservatorio esta cheio")
	end
end

