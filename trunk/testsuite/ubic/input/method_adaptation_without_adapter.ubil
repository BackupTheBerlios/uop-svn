entity start
	def start()
		element k = kernel.new()
		if (identity.device.images == true)
			k.run_show_images()
		else if (identity.device.images == false)
			k.run_no_images()
		end
	end
end

entity Kernel
	def run_show_images()
		io.writeln( "Exibe imagens !!!" )
	end
	def runNoImages()
		io.writeln( "Nao exibe imagens !!!" )
	end
end

